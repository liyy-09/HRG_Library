<resources>

    <!-- Base application theme. -->
    <style name="com_AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/com_colorPrimary</item>
        <item name="colorPrimaryDark">@color/com_colorPrimaryDark</item>
        <item name="colorAccent">@color/com_colorAccent</item>
    </style>

    <style name="com_MyDialog" parent="android:style/Theme.Dialog">
        <item name="android:background">@color/com_white_overlay</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
    </style>

</resources>
